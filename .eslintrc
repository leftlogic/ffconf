{
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {},
  "rules": {
    "camelcase": 2,
    "curly": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    semi: ["error", "always"],
    "no-unreachable": "error",
    "no-caller": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "strict": 0,
    "no-unused-vars": 2,
    "no-else-return": 2,
    "space-before-function-paren": ["error", { "anonymous": "always", "named": "never" }]
  }
}
