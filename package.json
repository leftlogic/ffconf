{
  "name": "ffconf",
  "version": "1.1.0",
  "description": "Root for ffconf",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": "10"
  },
  "scripts": {
    "update": "node ./tweets.js",
    "server:dev": "PORT=7000 nodemon ./graphql",
    "server": "PORT=7000 node ./graphql",
    "dev": "eleventy --serve",
    "redirects": "node ./redirects.js",
    "build": "eleventy && npm run redirects && test -e dist/index.html",
    "postbuild": "now --token=$NOW_TOKEN && now alias --token $NOW_TOKEN",
    "now-build": "true",
    "now-start": "node ./graphql"
  },
  "author": "Remy Sharp",
  "license": "MIT",
  "parked": {
    "@remy/ffconf2009": "^1.1.0",
    "@remy/ffconf2010": "^1.1.0",
    "@remy/ffconf2011": "^1.1.0",
    "@remy/ffconf2012": "^1.1.1",
    "@remy/ffconf2013": "^1.1.0",
    "@remy/ffconf2014": "^1.1.0",
    "@remy/ffconf2015": "^1.1.0",
    "@remy/ffconf2016": "^1.5.0",
    "@remy/ffconf2017": "^1.2.3",
    "@remy/ffconf2018": "1.2.15",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "graphql-request": "^1.4.0",
    "jwt-decode": "^2.2.0",
    "lru-cache": "^4.1.3",
    "vhost": "^3.0.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "graphql-request": "^1.8.2",
    "undefsafe": "^2.0.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "markdown-it-named-headings": "^1.1.0",
    "@11ty/eleventy-plugin-rss": "^1.0.6",
    "apollo-server-express": "^2.8.1",
    "apollo-server-testing": "^2.2.0",
    "date-fns": "^1.30.1",
    "cheerio": "^1.0.0-rc.3",
    "eslint": "^6.5.1",
    "node-fetch": "^2.6.0",
    "now": "^16.1.1"
  }
}
